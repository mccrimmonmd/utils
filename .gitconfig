[user]
  email = saturnhs@gmail.com
  name = Malcolm McCrimmon
[pull]
  ff = only
[alias]
  cg = config --global
  rebasem = rebase main --rebase-merges
  mergem = merge main
  st = status
  sdiff = diff --staged
  ci = commit
  ca = commit --all
  cim = commit -m
  cam = commit --all -m
  sub = restore --staged
  undo = revert HEAD
  undo-soft = revert HEAD
  undo-hard = reset --mixed HEAD~
  stashl = stash list
  stashs = stash show
  stashd = stash show --patch
  unstash = stash pop
  unst = stash pop
  br = branch
  ba = branch --all
  si = switch
  sw = switch
  sb = switch -
  sm = switch main
  nb = switch -c
  su = push --set-upstream origin HEAD
  ap = add --patch
  fe = fetch --all
  pr = fetch --prune
  delete-remote = push -d origin
  low = log -5
  loo = log --oneline
  lo = log --oneline -20
  gloo = log --oneline --graph
  glo = log --oneline --graph -20
  lop = log --patch
  op = log --patch -1
  
