[pull]
  ff = only
[alias]
  cg = config --global
  gconf = config --global
  rebasem = rebase main --rebase-merges
  mergem = merge main
  st = status
  ci = commit
  ca = commit --all
  cam = commit -am
  br = branch
  ba = branch --all
  si = switch
  sb = switch -
  sm = switch main
  nb = switch -c
  su = push --set-upstream origin
  ap = add -p
  fe = fetch --all
  pr = fetch --prune
  loo = log --oneline
  lo = log --oneline -20
  