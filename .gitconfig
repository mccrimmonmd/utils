[user]
  email = saturnhs@gmail.com
  name = Malcolm McCrimmon
[pull]
  ff = only
[alias]
  cg = config --global
  mig = remote set-url origin
  rebasem = rebase main --rebase-merges
  mergem = merge main
  st = status
  dif = diff -- . ':(exclude)package-lock.json'
  sdiff = diff --staged
  sdif = diff --staged -- . ':(exclude)package-lock.json'
  ci = commit
  ca = commit --all
  cim = commit -m
  cam = commit --all -m
  sub = restore --staged
  undo = revert HEAD
  undo-soft = revert --no-commit HEAD
  undo-hard = reset --mixed HEAD~
  undo-range = revert --no-commit
  stashl = stash list
  stashs = stash show
  stashd = stash show --patch
  unstash = stash pop
  unst = stash pop
  br = branch
  ba = branch --all
  si = switch
  sb = switch -
  sm = switch main
  nb = switch -c
  ch = checkout
  su = push --set-upstream origin HEAD
  ap = add --patch
  fe = fetch --all
  pr = fetch --prune
  delete-local = branch -d
  delete-remote = push -d origin
  gol = log --reverse
  low = log -5
  loo = log --oneline
  ool = log --oneline --reverse
  lo = log --oneline -20
  gloo = log --oneline --graph
  glo = log --oneline --graph -20
  lop = log --patch
  pol = log --patch --reverse
  op = log --patch -1
  
