[user]
  email = malcolm.mccrimmon@dish.com
  name = Malcolm McCrimmon
[pull]
  ff = only
[core]
  autocrlf = true
  editor = 'C:\\Program Files (x86)\\Notepad++\\notepad++.exe' -multiInst -notabbar -nosession -noPlugin
[alias]
  cg = config --global
  mig = remote set-url origin
  rebasem = rebase master --rebase-merges
  mergem = merge master
  st = status
  dif = diff -- . ':(exclude)package-lock.json'
  sdiff = diff --staged
  sdif = diff --staged -- . ':(exclude)package-lock.json'
  ci = commit
  ca = commit --all
  cim = commit -m
  cam = commit --all -m
  sub = restore --staged
  undo = revert HEAD
  undo-soft = revert --no-commit HEAD
  undo-hard = reset HEAD~
  undo-range = revert --no-commit
  pusht = push --follow-tags
  stashl = stash list
  stashs = stash show
  stashd = stash show --patch
  stashst = stash --staged
  taag = tag -a -m ''
  unstash = stash pop
  unst = stash pop
  br = branch
  ba = branch --all
  si = switch
  sb = switch -
  sm = switch master
  nb = switch -c
  ch = checkout
  su = push --set-upstream origin HEAD
  ap = add --patch
  fa = fetch --all
  pr = fetch --prune
  fe = fetch --all --prune
  prt = fetch --prune --prune-tags
  delete-local = branch -d
  delete-remote = push -d origin
  gol = log --reverse
  low = log -5
  loo = log --oneline
  ool = log --oneline --reverse
  lo = log --oneline -16
  gloo = log --oneline --graph
  glo = log --oneline --graph -20
  lop = log --patch
  pol = log --patch --reverse
  op = log --patch -1

