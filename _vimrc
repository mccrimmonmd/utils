unlet! skip_defaults_vim
source $VIMRUNTIME/defaults.vim

" let mapleader = ","

set relativenumber

inoremap jj <ESC>

" these don't work?
" nnoremap ; :
" cnoreabbrev H vert bo h
" let &t_SI = "\e[0 q"

set hlsearch
set ignorecase
set smartcase

set shiftwidth=2 smarttab
set expandtab
set tabstop=8 softtabstop=0
